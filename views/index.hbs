<script>
function getOkuResponse() {
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
      document.getElementById("GelenData").innerHTML =
      this.responseText;
    }
  };
  xhttp.open("GET", "/oku", true);
  xhttp.send();
}
